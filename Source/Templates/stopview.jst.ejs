<h2 class="stop-title">
    <%= window.NB.Utilities.stopTitle(get('code'), get('name')) %>
</h2>

<div class="card"></div>

<% if (get('hasAdditionalStops')) { %>
<div class="additional-stops">
    <button>
        More Buses at #<%= get('code') %>

        <img src="/static/img/next.png" class="img-next"/>
    </button>
</div>
<% } %>

